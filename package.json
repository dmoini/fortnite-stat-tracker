{
  "name": "giphy-sample",
  "private": true,
  "version": "0.0.0",
  "description": "A web front end sample",
  "main": "",
  "scripts": {
    "start": "superstatic",
    "test": "eslint '**/*.js' && ./node_modules/karma/bin/karma start --single-run"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.0.0",
    "jasmine-core": "latest",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "latest",
    "karma-coverage": "latest",
    "karma-firefox-launcher": "latest",
    "karma-fixture": "latest",
    "karma-html2js-preprocessor": "latest",
    "karma-jasmine": "^2.0.1",
    "karma-jquery": "latest",
    "karma-sinon": "latest",
    "sinon": "^7.3.1",
    "superstatic": "^6.0.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jasmine": true,
      "jquery": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "fixture": true,
      "sinon": true
    },
    "rules": {
      "no-extra-parens": 2,
      "curly": 2,
      "eqeqeq": 2,
      "no-eval": 2,
      "no-global-assign": 2,
      "no-lone-blocks": 2,
      "no-multi-spaces": 2,
      "no-return-assign": 2,
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ],
      "func-style": [
        2,
        "expression"
      ],
      "comma-spacing": 2,
      "computed-property-spacing": 2,
      "func-call-spacing": 2,
      "indent": [
        "error",
        2
      ],
      "key-spacing": 2,
      "keyword-spacing": 2,
      "semi-spacing": 2,
      "space-in-parens": 2,
      "space-infix-ops": 2,
      "space-unary-ops": 2,
      "spaced-comment": 2,
      "lines-around-comment": 2,
      "max-len": [
        2,
        128
      ],
      "new-parens": 2,
      "no-tabs": 2,
      "operator-linebreak": [
        2,
        "after"
      ],
      "no-console": 1,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "dependencies": {
    "just-extend": "^4.1.0",
    "minimist": "^1.2.5"
  }
}
